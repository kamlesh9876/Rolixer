# Application
NODE_ENV=test
PORT=3001

# Cypress Configuration
CYPRESS_BASE_URL=http://localhost:3001
CYPRESS_VERIFY_TIMEOUT=60000

# API Configuration
API_URL=http://localhost:3001/api

# Database
TEST_DATABASE_URL=postgresql://postgres:postgres@localhost:5432/store_rating_test

# Authentication
AUTH_SECRET=test-secret-key-change-in-production
JWT_SECRET=test-jwt-secret-change-in-production

# Test User Credentials
TEST_USER_EMAIL=test@example.com
TEST_USER_PASSWORD=Test@1234
TEST_ADMIN_EMAIL=admin@example.com
TEST_ADMIN_PASSWORD=Admin@1234

# Feature Flags
FEATURE_FLAG_NEW_UI=true

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100

# CORS
CORS_ORIGIN=http://localhost:3000

# Logging
LOG_LEVEL=debug

# Email (Mocked in tests)
SMTP_HOST=smtp.ethereal.email
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=test@example.com
SMTP_PASS=test-password

# File Uploads
UPLOAD_DIR=./uploads/test
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=image/jpeg,image/png,application/pdf

# Security
CSP_ENABLED=true
XSS_PROTECTION=true
HSTS_ENABLED=true

# Session
SESSION_SECRET=test-session-secret
SESSION_COOKIE_SECURE=false
SESSION_COOKIE_HTTP_ONLY=true

# Cache
CACHE_ENABLED=true
CACHE_TTL=300

# Monitoring
SENTRY_DSN=

# Analytics
GOOGLE_ANALYTICS_ID=

# Social Login (Mocked in tests)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
FACEBOOK_APP_ID=
FACEBOOK_APP_SECRET=

# Payment (Mocked in tests)
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=

# Map Services
MAPBOX_ACCESS_TOKEN=
GOOGLE_MAPS_API_KEY=
