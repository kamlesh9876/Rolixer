{
  "name": "store-rating-system",
  "version": "1.0.0",
  "description": "Store Rating System with NestJS and React",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.json",
    "test:ci": "jest --ci --runInBand --detectOpenHandles --forceExit",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:edge": "cypress run --browser edge",
    "cy:run:headless": "cypress run --headless",
    "cy:run:record": "cypress run --record --key <record-key>",
    "start:test": "cross-env NODE_ENV=test nest start --watch",
    "db:test:create": "NODE_ENV=test npx typeorm-ts-node-commonjs schema:sync -d test/test-db.config.ts",
    "db:test:drop": "NODE_ENV=test npx typeorm-ts-node-commonjs schema:drop -d test/test-db.config.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:test": "dotenv -e .env.test -- prisma migrate deploy"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^3.1.0",
    "@nestjs/core": "^9.4.3",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^4.0.0",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.4.3",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.19.123",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.37",
    "@types/supertest": "^2.0.16",
    "cross-env": "^7.0.3",
    "cypress": "^15.0.0",
    "dotenv": "^17.2.1",
    "dotenv-cli": "^7.2.1",
    "jest": "^29.7.0",
    "prisma": "^4.0.0",
    "start-server-and-test": "^2.0.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
